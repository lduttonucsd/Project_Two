<div class="row">
    <div class="col-8 offset-2">
      <h1 class="pageHeader">Hello {{user.email}}!</h1>
      <form class="clearfix mb-4" action="POST">
        <div class="form-group">
        
        </div>
        <div class="form-group">
          <label class="pageHeader" for="example-description">Description</label>
          <textarea class="form-control" id="example-description" aria-describedby="example-description"
            placeholder="describe yo self"></textarea>
        </div>
        <button id="submit" class="btn btn-primary float-right">Submit</button>
      </form>
      
      <ul id="example-list" class="list-group">
        {{#each user.Examples}}
        <li data-id="{{this.id}}" class="list-group-item">
          <a href="example/{{this.id}}">{{this.text}}</a>
          <button class="btn btn-danger float-right delete">ï½˜</button>
        </li>
        {{/each}}
      </ul>
    </div>
</div>
<iframe id="mainstream" src="https://player.twitch.tv/?channel=overwatchleague&muted=true" height="<height>"
      width="<width>" frameborder="<frameborder>" scrolling="<scrolling>" allowfullscreen="<allowfullscreen>">
    </iframe>